{
  "general": {
    "max_log_level": "INFO"
  },
  "git": {
    "project_dirs": [
      "$HOME"
    ],
    "excluded_dirs": [
      "$HOME/.local/share",
      "$HOME/Library",
      "$HOME/.tmux",
      "$HOME/.emacs.d",
      "$HOME/.cargo"
    ],
    "extra_fd_args": ""
  },
  "git_remote": {
    "repos_path": "$HOME"
  },
  "scratchpad": {
    "default_name": "scratch"
  },
  "tmux": {
    "extra_options": ""
  }
}
